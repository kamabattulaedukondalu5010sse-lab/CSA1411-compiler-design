#include <stdio.h>
#include <ctype.h>

int main() {
    char s[100];
    int i, num;

    printf("Enter the string: ");
    scanf("%[^\n]", s);

    printf("\nIdentifiers: ");
    for (i = 0; s[i]; i++) {
        if (isalpha(s[i]))
            printf("%c ", s[i]);
    }

    printf("\nConstants: ");
    for (i = 0; s[i]; i++) {
        if (isdigit(s[i])) {
            num = 0;
            while (isdigit(s[i])) {
                num = num * 10 + (s[i] - '0');
                i++;
            }
            printf("%d ", num);
        }
    }

    printf("\nOperators: ");
    for (i = 0; s[i]; i++) {
        if (s[i]=='+' || s[i]=='-' || s[i]=='*' || s[i]=='=')
            printf("%c ", s[i]);
    }

    return 0;
}

