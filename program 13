#include <stdio.h>
#include <string.h>

int main()
{
    char string[50];
    int i, aCount = 0, bCount = 0;
    int flag = 1;

    printf("The grammar is: S -> aS | Sb | ab\n");
    printf("Enter the string to be checked: ");

    fgets(string, sizeof(string), stdin);

    // Remove newline
    string[strcspn(string, "\n")] = '\0';

    /* Count a's and b's */
    for (i = 0; string[i] != '\0'; i++)
    {
        if (string[i] == 'a')
            aCount++;

        else if (string[i] == 'b')
            bCount++;

        else
        {
            flag = 0;
            break;
        }
    }

    /* Check grammar condition: a^n b^n (at least one a and one b) */
    if (flag == 1 && aCount >= 1 && bCount >= 1 && aCount == bCount)
        printf("String is accepted\n");
    else
        printf("String is not accepted\n");

    return 0;
}

OUTPUT:
The grammar is: S -> aS | Sb | ab
Enter the string to be checked: abb
String is not accepted
